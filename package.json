  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "cypress": "^10.4.0",
    "cypress-jest-adapter": "^0.1.1",
    "jsdom": "^20.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vitest": "^0.21.1",
    "vue-tsc": "^0.39.5"
  }
}
